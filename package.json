{
  "name": "animeclient",
  "version": "1.2.0",
  "description": "anilinkz client for windows",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . AnimeClient --overwrite --asar --platform=win32 --arch=ia32 --icon=icon.png --prune=true --out=release-builds",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "wagyourtail",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-packager": "^12.1.0"
  },
  "dependencies": {
    "jsdom": "^12.0.0",
    "request": "^2.88.0"
  }
}
